    <!-- http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#Manager -->

    <owl:Class rdf:about="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#Manager">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#Employee"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#managesDepartment"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#Department"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#managesEmployee"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#Employee"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dmitry/ontologies/2025/11/corporate_hierarchy#Employee"/>
        <rdfs:label xml:lang="ru">Менеджер</rdfs:label>
